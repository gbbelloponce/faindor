{
  "name": "faindor",
  "packageManager": "bun@1.2.1",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "prepare": "husky",
    "dev": "bun --filter '*' dev",
    "lint": "bunx biome check --write ./apps/",
    "clean": "rm -rf node_modules/ bun.lock"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "husky": "^9.1.7"
  },
  "trustedDependencies": [
    "@biomejs/biome",
    "esbuild"
  ]
}